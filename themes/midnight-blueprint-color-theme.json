      ],
      "settings": {
        "foreground": "#c3e88d"
      }
    },
    {
      "name": "Deleted",
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#ff5370"
      }
    },
    {
      "name": "Changed",
