      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#f9e79f"
      }
    },
    {
      "name": "CSS Classes",
      "scope": [
        "entity.other.attribute-name.class"
      ],
      "settings": {
        "foreground": "#f9e79f"
      }
    },
    {
